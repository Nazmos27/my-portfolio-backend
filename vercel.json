{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "PORT": "5000",
      "USER_NAME": "@user_name",
      "USER_PASS": "@user_pass",
      "ACCESS_TOKEN": "@access_token"
    }
  }
  